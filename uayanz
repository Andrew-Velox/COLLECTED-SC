	nazri_ganteng(f'└─ Your Ip  : {ip}')
	print(f'└─ Github   : Asep Yusup')
	nazri_ganteng('=' * 40)
	cetak('[bold green] [•]1. Crack Public\n [•]0. Exit[bold green]')
